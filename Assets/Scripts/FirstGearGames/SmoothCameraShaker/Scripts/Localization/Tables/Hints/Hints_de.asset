  m_TableData:
  - m_Id: 52114407424
    m_Localized: "Du kannst deine <color=#D47F36>H\xC4NDE</color> <color=#C4A000>VERWENDEN</color>
      mit <color=#6FB7D6>\"$primary_key\"</color>, um die <color=#9B111E>KISTEN</color>
      zu zerbrechen."
    m_Metadata:
      m_Items: []
  - m_Id: 57227675353088
    m_Localized: "Um die <color=#FFB45B>Laterne</color> zu entz\xFCnden, musst du
      <color=#E688FF>Streichh\xF6lzer</color> und <color=#E688FF>Gas\xF6l</color>
      finden."
    m_Metadata:
      m_Items: []
  - m_Id: 68442363445248
    m_Localized: "Dr\xFCcken Sie eine Taste, um fortzufahren..."
    m_Metadata:
      m_Items: []
  - m_Id: 148327882735616
    m_Localized: "Du kannst dein <color=#735D94>Inventar</color> <color=#C4A000>\xD6FFNEN</color>,
      indem du <color=#6FB7D6>\"$inv_key\"</color> dr\xFCckst. In deinem <color=#735D94>Inventar</color>
      kannst du Gegenst\xE4nde <color=#C4A000>Verwenden</color>, <color=#C4A000>Untersuchen</color>,
      <color=#C4A000>Wegwerfen</color> oder <color=#C4A000>Kombinieren</color>! Vergiss
      nicht, deine <color=#D47F36>Gaslampe</color> mit <color=#3A6B35>Gas\xF6l</color>
      aufzuf\xFCllen."
    m_Metadata:
      m_Items: []
  - m_Id: 1828277039841280
    m_Localized: oder
    m_Metadata:
      m_Items: []
  - m_Id: 1841791259295744
    m_Localized: "Durch Dr\xFCcken der Taste <color=#6FB7D6>\"$inter_key\"</color>
      kannst du <color=#C4A000>ERWERBEN</color> <color=#735D94>Gegenst\xE4nde</color>
      oder mit der <color=#9B111E>Umgebung</color> <color=#C4A000>INTERAGIEREN</color>."
    m_Metadata:
      m_Items: []
  - m_Id: 2151958932799488
    m_Localized: "Indem du deine <color=#AA4074>Aufmerksamkeit Fokussierst</color>,
      kannst du <color=#735D94>Objekte Erkennen</color>, die f\xFCr eine <color=#C4A000>Interaktion</color>
      offen sind. Um dies zu tun, <color=#C4A000>VERWENDE</color> die <color=#6FB7D6>\"$scan_env\"</color>-Taste.
      Bedenke jedoch, dass diese F\xE4higkeit deine <color=#7A85FF>Ausdauer</color>
      stark <color=#C4A000>VERBRAUCHEN</color> wird."
    m_Metadata:
      m_Items: []
  - m_Id: 21320435609100288
    m_Localized: "Du kannst deine <color=#735D94>Gegenst\xE4nde</color> aus deinem
      <color=#735D94>Inventar</color> <color=#C4A000>ABLEGEN</color>."
    m_Metadata:
      m_Items: []
